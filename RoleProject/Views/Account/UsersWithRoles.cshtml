@model IEnumerable<RoleProject.View_Model.Users_in_Role_ViewModel>

@{
    ViewBag.Title = "Users With Roles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

        <table class="table table-hover ">
            <thead>
                <tr>
                    <td><b>Username</b></td>
                    <td><b>Email</b></td>
                    <td><b>City</b></td>
                    <td><b>Street</b></td>
                    <td><b>Phone Number</b></td>         
                    <td><b>Roles</b></td>
                </tr>
            </thead>
            @foreach (var user in Model)
            {
        <tr>
            <td>@user.Username</td>
            <td>@user.Email</td>
            <td>@user.city</td>
            <td>@user.street</td>
            <td>@user.phone_number</td>
            <td>@user.Role</td>

        </tr>

            }
        </table>
    


