@model RoleProject.Models.Car
@{
    ViewBag.Title = "Recive";
}

<h2>Recive</h2>
@using (Html.BeginForm("Recive", "Cars", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <div class="form-group">
        @Html.LabelFor(model => model.Start_Book_Date, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            <input type="datetime" name="Start_Book_Date" id="Start_Book_Date" class="form-control" />
            @Html.ValidationMessageFor(model => model.Start_Book_Date, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.End_Book_Date, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            <input type="datetime" name="End_Book_Date" id="End_Book_Date" class="form-control" />
            @Html.ValidationMessageFor(model => model.End_Book_Date, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.HiddenFor(model => model.Car_Id)
          
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Recive" class="btn btn-default" />
        </div>
    </div>

}
<div>
    @Html.ActionLink("Back to List", "List_Of_all")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}